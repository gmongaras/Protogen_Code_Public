// Helper: Convert RGB888 to RGB565
uint16_t rgb888_to_rgb565(uint8_t r, uint8_t g, uint8_t b) {
    uint16_t r5 = (r >> 3) & 0x1F;
    uint16_t g6 = (g >> 2) & 0x3F;
    uint16_t b5 = (b >> 3) & 0x1F;
    return (r5 << 11) | (g6 << 5) | b5;
}

// Helper: HSV to RGB conversion
void hsv_to_rgb(float h, float s, float v, uint8_t& r, uint8_t& g, uint8_t& b) {
    float c = v * s;
    float x = c * (1 - std::fabs(fmod(h * 6, 2) - 1));
    float m = v - c;

    float r_, g_, b_;
    if (h < 1.0 / 6) {
        r_ = c; g_ = x; b_ = 0;
    } else if (h < 2.0 / 6) {
        r_ = x; g_ = c; b_ = 0;
    } else if (h < 3.0 / 6) {
        r_ = 0; g_ = c; b_ = x;
    } else if (h < 4.0 / 6) {
        r_ = 0; g_ = x; b_ = c;
    } else if (h < 5.0 / 6) {
        r_ = x; g_ = 0; b_ = c;
    } else {
        r_ = c; g_ = 0; b_ = x;
    }

    r = static_cast<uint8_t>((r_ + m) * 255);
    g = static_cast<uint8_t>((g_ + m) * 255);
    b = static_cast<uint8_t>((b_ + m) * 255);
}

// Compute rainbow color and return RGB565
uint16_t rainbow_rgb565(int t, int max_) {
    if (t < 0 || t > max_) return 0;

    float hue = static_cast<float>(t) / max_;  // hue ∈ [0, 1]
    uint8_t r, g, b;
    hsv_to_rgb(hue, 1.0f, 1.0f, r, g, b);
    return rgb888_to_rgb565(r, g, b);
}

// Generate the rainbow bitmap
void generate_rainbow_bitmap(uint16_t bitmap[HEIGHT][WIDTH2], uint16_t t_vals[HEIGHT][WIDTH2], uint16_t bitmap_ref[HEIGHT][WIDTH2], bool increment=false) {
    for (int y = 0; y < HEIGHT; ++y) {
        for (int x = 0; x < WIDTH2; ++x) {
            // Only put pixels on the reference image
            if (bitmap_ref[y][x] == 0x0) {
              continue;
            }
            // Increment t values
            if (increment) {
              t_vals[y][x] = (t_vals[y][x] - 1) % WIDTH2;
            }
            // Change bitmap
            bitmap[y][x] = rainbow_rgb565(t_vals[y][x], WIDTH2);
        }
    }
}

// t values initialization
void get_t_vals_init(uint16_t t_vals[HEIGHT][WIDTH2]) {
  for (int y = 0; y < HEIGHT; ++y) {
        for (int x = 0; x < WIDTH2; ++x) {
            int t = x;
            t_vals[y][x] = t;
        }
    }
}







// Function to initialize the matrix from a bitmap
void initMatrixFromBitmap(uint16_t bitmap[HEIGHT][WIDTH2], bool initialize_pix) {
  // Update the matrix screen with pixels
  for (int y = 0; y < HEIGHT; y++) {
    for (int x = 0; x < WIDTH2; x++) {
      // Write the pixel to the matrix
      uint16_t pixval = bitmap[y][x];
      // matrix.drawPixel(WIDTH2-x-1, HEIGHT-y, pixval);
      // matrix.drawPixel(2*WIDTH2-x-1, HEIGHT-y, pixval);
      matrix.drawPixel(WIDTH2-x, HEIGHT-y-1, pixval);
      matrix.drawPixel(WIDTH2+x, HEIGHT-y-1, pixval);

    }
  }
  matrix.show(); // Display the bitmap on the matrix
}









void setup() {
  // Initialize Serial Monitor
  Serial.begin(921600);

  // Initialize the matrix
  ProtomatterStatus status = matrix.begin();

  // Get t values
  get_t_vals_init(t_vals);
  generate_rainbow_bitmap(new_bitmap, t_vals, bitmap);


  initMatrixFromBitmap(new_bitmap, false);
  Serial.printf("Protomatter begin() status: %d\n", status);
}

